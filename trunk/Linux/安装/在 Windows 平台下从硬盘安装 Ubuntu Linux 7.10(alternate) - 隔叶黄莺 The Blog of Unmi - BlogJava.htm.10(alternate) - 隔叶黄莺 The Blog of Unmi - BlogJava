<SCRIPT>
    var dq = document.getElementById("dq");
    var db = document.getElementById("db");
    var da = document.getElementById("da");

    function findict(evt){
      evt = evt ? evt : (window.event ? window.event : null);
     if(evt.srcElement==dq || evt.target==dq){
          if(evt.keyCode != 13) return;
          evt.keyCode = 9;
          //dq.focus();
      }
      dq.value = dq.value.replace(/^\s+|\s+$/,'')
      if(dq.value=="") return;
      da.href="http://dict.cn/search/?q="+dq.value;  
      da.click();
      dq.value = "";
   }
</SCRIPT>

