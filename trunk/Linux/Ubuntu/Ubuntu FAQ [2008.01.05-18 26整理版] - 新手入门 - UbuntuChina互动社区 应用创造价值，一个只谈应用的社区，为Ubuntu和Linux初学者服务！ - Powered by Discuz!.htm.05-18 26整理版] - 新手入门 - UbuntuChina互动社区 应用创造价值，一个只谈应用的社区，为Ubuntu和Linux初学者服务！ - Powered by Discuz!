		function modaction(action) {
			if(!action) {
				return;
			}
			if(!in_array(action, ['delpost', 'banpost'])) {
				window.location=('topicadmin.php?tid=5338&fid=8&action='+ action +'&sid=2yJp57');
			} else {
				document.modactions.action = 'topicadmin.php?action='+ action +'&fid=8&tid=5338&page=1;'
				document.modactions.submit();
			}
		}
	</script>
